{
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "UID", "placeholder": "Unique page ID" }
      },

      "hero_title": { "type": "Text", "config": { "label": "Hero Title" } },
      "hero_subtitle": { "type": "Text", "config": { "label": "Hero Subtitle" } },
      "hero_category": { "type": "Text", "config": { "label": "Hero Category" } },
      "hero_cta_text": { "type": "Text", "config": { "label": "Hero CTA Text" } },
      "hero_cta_link": { "type": "Link", "config": { "label": "Hero CTA Link" } },

      "gallery_images": {
        "type": "Group",
        "config": {
          "label": "Gallery Images",
          "fields": {
            "image": { "type": "Image", "config": { "label": "Image" } }
          }
        }
      },

      "about_heading": { "type": "Text", "config": { "label": "About Heading" } },
      "about_content": {
        "type": "StructuredText",
        "config": {
          "label": "About Content",
          "multi": "paragraph, heading1, heading2, heading3, strong, em, hyperlink, list-item, o-list-item"
        }
      },
      "about_image": { "type": "Image", "config": { "label": "About Image" } },
      "about_link": { "type": "Link", "config": { "label": "About Link" } },

      "projects": {
        "type": "Group",
        "config": {
          "label": "Projects",
          "fields": {
            "title": { "type": "Text", "config": { "label": "Project Title" } },
            "meta": { "type": "Text", "config": { "label": "Project Meta" } },
            "image1": { "type": "Image", "config": { "label": "Project Image 1" } },
            "image2": { "type": "Image", "config": { "label": "Project Image 2" } },
            "link": { "type": "Link", "config": { "label": "Project Link" } }
          }
        }
      },

      "counters": {
        "type": "Group",
        "config": {
          "label": "Counters",
          "fields": {
            "value": { "type": "Number", "config": { "label": "Counter Value" } },
            "label": { "type": "Text", "config": { "label": "Counter Label" } }
          }
        }
      },

      "video_url": { "type": "Text", "config": { "label": "Video URL" } },
      "video_subtitle": { "type": "Text", "config": { "label": "Video Subtitle" } },
      "video_title": { "type": "Text", "config": { "label": "Video Title" } },
      "video_description": { "type": "Text", "config": { "label": "Video Description" } },

      "services_section_title": { "type": "Text", "config": { "label": "Services Section Title" } },
      "services_section_subtitle": { "type": "Text", "config": { "label": "Services Section Subtitle" } },
      "services_icon": { "type": "Image", "config": { "label": "Services Icon" } },
      "services": {
        "type": "Group",
        "config": {
          "label": "Services",
          "fields": {
            "title": { "type": "Text", "config": { "label": "Service Title" } },
            "desc": { "type": "Text", "config": { "label": "Service Description" } },
            "category": {
              "type": "Group",
              "config": {
                "label": "Categories",
                "fields": { "name": { "type": "Text", "config": { "label": "Category Name" } } }
              }
            },
            "link": { "type": "Link", "config": { "label": "Service Link" } }
          }
        }
      },

      "cta_title": { "type": "Text", "config": { "label": "CTA Title" } },
      "cta_subtitle": { "type": "Text", "config": { "label": "CTA Subtitle" } },
      "cta_description": { "type": "Text", "config": { "label": "CTA Description" } },
      "cta_button_text": { "type": "Text", "config": { "label": "CTA Button Text" } },
      "cta_button_link": { "type": "Link", "config": { "label": "CTA Button Link" } },
      "cta_image": { "type": "Image", "config": { "label": "CTA Image" } },

      "footer_email": { "type": "Text", "config": { "label": "Footer Email" } },
      "footer_address_text": { "type": "Text", "config": { "label": "Footer Address Text" } },
      "footer_address_mapLink": { "type": "Link", "config": { "label": "Footer Address Map Link" } },
      "footer_phone": { "type": "Text", "config": { "label": "Footer Phone" } },
      "footer_social_links": {
        "type": "Group",
        "config": {
          "label": "Footer Social Links",
          "fields": {
            "label": { "type": "Text", "config": { "label": "Social Label" } },
            "url": { "type": "Link", "config": { "label": "Social URL" } }
          }
        }
      }
    }
  }
}
